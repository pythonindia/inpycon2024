<!-- partners -->
<section id="community">
    <div class="container">
        <div class="row align-items-center p-2 p-md-0">
            <div class="col-md-12 pt-5 pt-md-0 pb-5">
                <h2 class="title-2 text-center pb-5" data-aos="fade-up" data-aos-duration="1000">Community partners</h2>
            </div>
            {% for partner in partners %}
            <div class="col-md-3 col-6 plus-box">
                <div class="plusbox-left one">
                    <i class="fa-solid fa-plus"></i>
                </div>
                <div class="plusbox-right two">
                    <i class="fa-solid fa-plus"></i>
                </div>
                <img src="/image/{{ partner.image }}" alt="{{ partner.name }}" class="img-fluid">
            </div>
            {% endfor %}
        </div>
        <div class="row justify-content-center pt-5 pb-5 mt-md-5">
            <div class="col-lg-3  col-md-5 col-12 mb-5 mb-md-0 text-center text-md">
                <div class="ticket-btnlay"><a href="#">BECAME A PARTNER</a> </div>
            </div>
            <div class="col-lg-3 col-md-5 col-12 mb-5 mb-md-0 text-center text-md">
                <div class="ticket-btnlay"><a href="#">BECOME PROSPECTUS</a></div>
            </div>
        </div>
    </div>
</section>

